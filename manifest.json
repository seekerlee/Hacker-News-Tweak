{
  "name": "Hacker News Tweak",
  "version": "0.1",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Hacker News Tweak"
  },
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["https://news.ycombinator.com/*"],
      "css": ["content_style.css"],
      "js": ["jquery-2.0.2.min.js", "content_script.js"]
    }
  ]
}
